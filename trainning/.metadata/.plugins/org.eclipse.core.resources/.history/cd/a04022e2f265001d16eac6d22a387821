
public abstract class Graphique {
	
	//Operation metier 
	public abstract void afficher();
	public abstract void effacer();
	public abstract void deplacerDe(int x,int y);
	
	//Methode propre au patron 
	public boolean ajouter(Graphique);
	public boolean ajouter(Graphique);
	public boolean ajouter(Graphique);
}
