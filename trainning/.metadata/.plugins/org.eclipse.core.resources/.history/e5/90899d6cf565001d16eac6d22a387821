import java.*;

public class Terninal {
	public static void clear() {
		System.out.println("Ecran effacer");
	}
	public static void charLigneColonne(int l,int c,char motif) {
		System.out.println("Ecriture du motif en ligne l et colonne c");
		
	}
	public static void stringLigneColonne(int l,int c,string chaine) {
		System.out.println("Ecriture la ligne en l et la colonne en c en partant du point haut gauche");
		
	}

}
