
public abstract class Graphique {
	
	//Operation metier 
	public abstract void afficher();
	public abstract void effacer();
	public abstract void deplacerDe(int x,int y);
	
	//Methode propre au patron 
	public abstract boolean ajouter(Graphique unGraphique);
	public abstract boolean supprimer(Graphique unGraphique);
	public abstract boolean existe(Graphique unGraphique);

	//Methode complementaire 
	public boolean estComposee() {
		return false;
	}
	
}
