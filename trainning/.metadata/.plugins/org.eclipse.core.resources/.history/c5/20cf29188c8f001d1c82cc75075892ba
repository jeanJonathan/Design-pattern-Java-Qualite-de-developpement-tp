
public class TemplateClass {

	public TemplateClass() {
		// TODO Auto-generated constructor stub
	}
	/*Ce algorithme depant de d'autres methodes*/
	public int templateMethode() {
		int a = etape1();
		int somme = 0;
		for(int i = 0 ; i <= a; i++) {
			somme += etape2();
		}
		return somme;
	}
	public abstract int etape1();
	public abstract int etape2();
}
