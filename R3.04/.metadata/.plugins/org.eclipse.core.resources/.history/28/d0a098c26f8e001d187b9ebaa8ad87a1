
public class Utilisateur extends Utilisateurs{


	public Utilisateur(ServeurDeChat unServeurDeChat) {
		super(unServeurDeChat);
	}

	public void recevoirMessage(String message,Utilisateurs unUtilisateur) {
		System.out.println("L'utilisateur a recu de la part de "+unUtilisateur.getNom() +"le message suivant: "+message);
	}	
}
 