
public abstract class DecorateurDeComposant implements IComposant {

	private IComposant composantDecorer;
	
	public DecorateurDeComposant(IComposant unComposant) {
		composantDecorer = unComposant;
	}
	public void setComposantDecorer(IComposant unComposant) {
		composantDecorer = unComposant;
	}
	
	

}
