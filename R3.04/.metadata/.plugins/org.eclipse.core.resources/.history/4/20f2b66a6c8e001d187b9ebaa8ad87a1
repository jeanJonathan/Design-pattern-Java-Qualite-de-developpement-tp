
public class Utilisateur extends Utilisateurs{


	public Utilisateur(ServeurDeChat unServeurDeChat) {
		super(unServeurDeChat);
	}

	public void recevoirMessage(String message,Utilisateurs auteur) {
		System.out.println("L'utilisateur a recu de la part de "+auteur.getNom() +"le message suivant: "+message);
	}	

}
 