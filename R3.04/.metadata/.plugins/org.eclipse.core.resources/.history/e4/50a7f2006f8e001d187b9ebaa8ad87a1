
public interface ServeurDeChat {

	public abstract void diffuserMessageDe(String message,Utilisateurs utilisateur);
	public abstract boolean ajouterUtilisateur(Utilisateurs unUtilisateur);
	public abstract boolean retirerUtilisateur(Utilisateurs unUtilisateur);
	public abstract boolean existeUtilisateur(Utilisateurs unUtilisateur);
	

}

