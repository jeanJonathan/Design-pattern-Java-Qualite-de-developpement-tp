
public class Utilisateur extends Utilisateurs{

	public Utilisateur() {
		// TODO Auto-generated constructor stub
	}

	public void recevoirMessage(String message) {
		System.out.println("L'utilisateur "+getNom()+" a recu le message suivant: "+message);
	}
	private String nom;
	public String getNom() {
		return nom;
	}
	
	
}
