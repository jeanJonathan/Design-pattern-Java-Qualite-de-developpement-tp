
public interface ServeurDeChat {

	public abstract void diffuserMessageDe(String message,Utilisateurs utilisateur);
	public abstract void ajouterUtilisateur(Utilisateur unUtilisateur);
	public abstract void retirerUtilisateur(Utilisateur unUtilisateur);
	public abstract void existeUtilisateur(Utilisateur unUtilisateur);
	

}

