
public class Utilisateur extends Utilisateurs{


	public Utilisateur(ServeurDeChat unServeurDeChat) {
		super(unServeurDeChat);
	}

	public void recevoirMessage(String message) {
		System.out.println("L'utilisateur en question a recu de"+ getNom() +" le message suivant: "+message);
	}	
	String name;
	public void setNom(String nom) {
		name = nom;
	}
	public String getNom() {
		return name;
	}
}
